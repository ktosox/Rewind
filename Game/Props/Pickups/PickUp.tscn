		COLOR.a = 0.0;
		}
}		
	
void vertex() {	
	VERTEX.x *= (abs(sin(2.0*TIME) )+6.0)/7.0;
	VERTEX.y *=(abs(cos(2.0*TIME) )+6.0)/7.0;
}	"

